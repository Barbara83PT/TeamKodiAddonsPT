<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.djaythemes" name="Djay Themes" version="1.0.1" provider-name="Djay Builds">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Djay Themes</summary>
    <description lang="en">Welcome to the Djay Themes where you can get all the latest Djay Themes for kodi and spmc</description>
    <platform>all</platform>
  </extension>
</addon>